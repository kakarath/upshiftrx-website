{
  "functions": {
    "src/app/api/*/route.ts": {
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "https://upshiftrx.ai/:path*",
      "destination": "https://www.upshiftrx.ai/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=60, stale-while-revalidate=300"
        }
      ]
    }
  ]
}